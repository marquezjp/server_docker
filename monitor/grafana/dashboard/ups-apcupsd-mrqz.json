{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.5.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "gauge", "name": "Gauge", "version": "" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "table", "name": "Table", "version": "" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.9.0" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "name": "ONBATT events",
        "type": "tags",
        "datasource": "${DS_PROMETHEUS}",
        "enable": true,
        "expr": "apcupsd_ups_status{status=\"ONBATT\",instance=~\"$instance\"} == 1",
        "iconColor": "rgba(255, 96, 96, 1)",
        "titleFormat": "UPS em bateria (ONBATT)",
        "textFormat": "UPS entrou em ONBATT"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "iteration": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "UPS Status",
      "description": "Mostra ONLINE / ONBATT / COMMLOST. O painel exibe o NOME da série (status) cujo valor é 1.",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "exemplar": false,
          "expr": "max by (status) (apcupsd_ups_status{instance=~\"$instance\",hostname=~\"$hostname\"} == 1)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "name",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "useBackground": true
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "1": { "text": "ONLINE" } } },
            { "type": "value", "options": { "0": { "text": "" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "title": "Bateria (%)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "apcupsd_battery_charge_percent{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 25 },
              { "color": "green", "value": 50 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "gauge",
      "title": "Autonomia (min)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "apcupsd_battery_time_left_seconds{instance=~\"$instance\",hostname=~\"$hostname\"} / 60",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 10 },
              { "color": "green", "value": 20 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "gauge",
      "title": "Carga do UPS (%)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "apcupsd_ups_load_percent{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "timeseries",
      "title": "Tensão de entrada (V)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "apcupsd_line_volts{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "legendFormat": "LINEV",
          "refId": "A"
        },
        {
          "expr": "apcupsd_line_nominal_volts{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "legendFormat": "NOMINAL",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "volt",
          "decimals": 1
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "type": "timeseries",
      "title": "Tensão da bateria (V)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "apcupsd_battery_volts{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "legendFormat": "BATTV",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "volt", "decimals": 1 },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Temperatura da bateria (°C)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "apcupsd_battery_temperature_celsius{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "legendFormat": "TEMP",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "celsius", "decimals": 1 },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "table",
      "title": "Informações do UPS",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "apcupsd_ups_info{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "refId": "A",
          "format": "table"
        },
        {
          "expr": "apcupsd_apcupsd_nominal_power_watts{instance=~\"$instance\",hostname=~\"$hostname\"}",
          "refId": "B",
          "format": "table"
        }
      ],
      "transformations": [
        { "id": "labelsToFields", "options": {} },
        { "id": "merge", "options": {} },
        { "id": "organize", "options": {
          "excludeByName": {},
          "indexByName": {},
          "renameByName": {
            "model": "Modelo",
            "hostname": "Hostname",
            "ups_name": "UPS Name",
            "Value": "Valor",
            "apcupsd_apcupsd_nominal_power_watts": "Potência Nominal (W)"
          }
        }}
      ],
      "options": {
        "showHeader": true,
        "footer": { "enablePagination": false, "fields": "", "reducer": ["sum"] }
      },
      "fieldConfig": { "defaults": {}, "overrides": [] }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["ups", "apcupsd", "prometheus"],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Datasource",
        "query": "prometheus",
        "current": { "selected": true },
        "hide": 0
      },
      {
        "type": "query",
        "name": "instance",
        "label": "Instance",
        "datasource": "${DS_PROMETHEUS}",
        "includeAll": false,
        "multi": false,
        "query": "label_values(apcupsd_ups_info, instance)",
        "refresh": 1,
        "sort": 1,
        "current": { "selected": true, "text": "All", "value": ".*" },
        "regex": ""
      },
      {
        "type": "query",
        "name": "hostname",
        "label": "Hostname",
        "datasource": "${DS_PROMETHEUS}",
        "includeAll": true,
        "multi": false,
        "query": "label_values(apcupsd_ups_info{instance=~\"$instance\"}, hostname)",
        "refresh": 1,
        "sort": 1,
        "current": { "selected": true, "text": "All", "value": ".*" },
        "regex": ""
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","15s","30s","1m","5m"] },
  "timezone": "",
  "title": "UPS (APCUPSD) - MRQZ",
  "uid": "ups-apcupsd-mrqz",
  "version": 1,
  "weekStart": ""
}
